class Solution {
    public int maxProduct(int[] nums) {

       int temp =0; 

        for(int i=0;i<=nums.length-1;i++){  
            for(int j=i+1;j<=nums.length-1;j++){ 

                int product = ((nums[i]-1)*(nums[j]-1)); 
            
                if(product> temp){  
                   temp = product;
                }
            }
        }
        return temp;
    }
}
